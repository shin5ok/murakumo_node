<pool type='netfs'>
  <name>[% uuid -%]</name>
  <uuid>[% uuid -%]</uuid>
  <capacity>0</capacity>
  <allocation>0</allocation>
  <available>0</available>
  <source>
    <host name='[% host -%]'/>
    <dir path='[% export_path -%]'/>
    <format type='auto'/>
  </source>
  <target>
    <path>[% mount_path -%]</path>
    <permissions>
      <mode>0700</mode>
      <owner>-1</owner>
      <group>-1</group>
    </permissions>
  </target>
</pool>
